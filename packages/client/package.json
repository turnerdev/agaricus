{
  "name": "agaricus-client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage --passWithNoTests --watch",
    "start": "parcel serve src/index.html --port 8000",
    "clean": "rm -rf .parcel-cache dist"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/src/style.css"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "hybrids": "^4.3.3",
    "parcel": "^2.0.0-beta.1",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^10.0.4",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.1.14"
  }
}
