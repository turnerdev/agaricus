{
  "name": "@agaricus/server",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npx nodemon --experimental-json-modules",
    "testold": "node --experimental-vm-modules node_modules/.bin/jest --coverage --watch",
    "test": "node_modules/.bin/jest --coverage --watch",
    "migrate": "npx knex migrate:up --esm"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "awilix": "^4.3.1",
    "csv-parse": "^4.14.1",
    "fastify": "^3.9.1",
    "fastify-multipart": "^3.3.1",
    "fastify-websocket": "^2.0.11",
    "knex": "^0.21.14",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.7",
    "babel-jest": "^26.6.3",
    "fastify-swagger": "^3.5.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "nodemon": "^2.0.6",
    "script": "^0.1.4"
  }
}
